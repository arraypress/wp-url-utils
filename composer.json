{
  "name": "arraypress/wp-url-utils",
  "version": "1.0.0",
  "description": "A lightweight WordPress library for comprehensive URL manipulation, validation, and connectivity checking with bulk operations support.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-url-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\URLUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-url-utils/issues",
    "source": "https://github.com/arraypress/wp-url-utils"
  },
  "keywords": [
    "wordpress",
    "url",
    "utilities",
    "validation",
    "sanitization",
    "utm-tracking",
    "url-manipulation",
    "http-checker",
    "arraypress"
  ]
}